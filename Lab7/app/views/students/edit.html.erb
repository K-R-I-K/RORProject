<h1>Edit</h1>
<%= form_for :student, url: student_path(@student), method: :patch do |f| %>
  <p>
    Lastname<br>
    <%= f.text_field :lastname %>
  </p>
  <p>
    Group<br>
    <%= f.text_field :group %>
  </p>
  <p>
    session id<br>
    <%= f.number_field :winter_session_id %>
  </p>
  <p>
    Marks
  </p>
  <p>
    Geometry<br>
    <%= f.number_field :geom, min: 1, max: 5 %>
  </p>
  <p>
    Algebra<br>
    <%= f.number_field :alg, min: 1, max: 5%>
  </p>
  <p>
    IT<br>
    <%= f.number_field :inf, min: 1, max: 5 %>
  </p>
  <%= f.submit "Update student" %>
<% end %>